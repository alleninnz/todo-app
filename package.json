{
  "name": "todo-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint . && echo '✅ Lint Check Passed'",
    "lint:fix": "eslint . --fix && echo '✅ Lint Fix Complete'",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\" --log-level silent && echo '✅ Format Complete'",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\" --log-level warn && echo '✅ Format Check Passed'",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fontsource-variable/roboto": "^5.2.8",
    "@hookform/resolvers": "^5.2.2",
    "@mui/icons-material": "^7.3.5",
    "@mui/material": "^7.3.5",
    "@mui/x-date-pickers": "^8.19.0",
    "dayjs": "^1.11.19",
    "ky": "^1.14.0",
    "notistack": "^3.0.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.66.1",
    "react-router": "^7.9.6",
    "react-router-dom": "^7.9.6",
    "zod": "^4.1.12",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.3",
    "@eslint/js": "^9.39.1",
    "@storybook/addon-a11y": "^10.0.8",
    "@storybook/addon-docs": "^10.0.8",
    "@storybook/addon-onboarding": "^10.0.8",
    "@storybook/addon-themes": "^10.0.8",
    "@storybook/addon-vitest": "^10.0.8",
    "@storybook/react-vite": "^10.0.8",
    "@tailwindcss/vite": "^4.1.17",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/browser-playwright": "^4.0.12",
    "@vitest/coverage-v8": "^4.0.12",
    "babel-plugin-react-compiler": "^1.0.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "eslint-plugin-storybook": "^10.0.8",
    "globals": "^16.5.0",
    "jsdom": "^27.2.0",
    "msw": "^2.12.2",
    "msw-storybook-addon": "^2.0.6",
    "playwright": "^1.56.1",
    "prettier": "^3.6.2",
    "storybook": "^10.0.8",
    "tailwindcss": "^4.1.17",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.47.0",
    "vite": "npm:rolldown-vite@7.2.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.12"
  },
  "pnpm": {
    "overrides": {
      "vite": "npm:rolldown-vite@7.2.3"
    },
    "onlyBuiltDependencies": [
      "@swc/core",
      "esbuild",
      "msw"
    ]
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
