  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fontsource-variable/roboto": "^5.2.8",
    "@hookform/resolvers": "^5.2.2",
    "@mui/icons-material": "^7.3.4",
    "@mui/material": "^7.3.4",
    "@mui/x-date-pickers": "^8.16.0",
    "dayjs": "^1.11.19",
    "ky": "^1.13.0",
    "notistack": "^3.0.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-form-hook": "^0.0.1",
    "react-router": "^7.9.5",
    "react-router-dom": "^7.9.5",
    "zod": "^4.1.12",
    "zustand": "^5.0.8"
