    "msw": "^2.11.6",
    "msw-storybook-addon": "^2.0.6",
    "playwright": "^1.56.1",
    "prettier": "^3.6.2",
    "storybook": "^10.0.2",
    "tailwindcss": "^4.1.16",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.2",
    "vite": "npm:rolldown-vite@7.1.14",
    "vitest": "^4.0.6"
  },
  "pnpm": {
    "overrides": {
      "vite": "npm:rolldown-vite@7.1.14"
    },
    "onlyBuiltDependencies": [
      "@swc/core",
      "esbuild",
      "msw"
    ]
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
